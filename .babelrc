{
  "presets": [
    ["es2015", { "modules": false }]
  ],
  "plugins": [
    [
      "component", [{
        "libraryName": "element-ui",
        "styleLibraryName": "theme-default"
      }]
    ],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    "transform-object-assign",
    "transform-object-rest-spread",
    "syntax-dynamic-import"
  ]
}
